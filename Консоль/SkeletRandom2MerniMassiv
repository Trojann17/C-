#include <iostream>
#include <stdlib.h>
#include <time.h>
using namespace std;
int main()
{
  srand(time(NULL));
  const int rad = 5;
  const int col = 5;
  int ar[rad][col];
  int i,g,result=0;
  for( i = 0; i < rad;i++){
    cout << "\n";
    for( g = 0; g < col; g++){
        ar[i][g] = rand()% (10 - (-10) + 1) -10;
        cout << ar[i][g]<<"\t";
    }
  }
  //TODO



    return 0;
}
